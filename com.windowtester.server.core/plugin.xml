<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.windowtester.server.core.Application">
         </run>
      </application>
   </extension>
   <extension
         id="WindowTester_Server_Core"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.windowtester.server.core.application"
            name="WindowTester Server">
         <property
               name="appName"
               value="WindowTester Server">
         </property>
      </product>
   </extension>

</plugin>
